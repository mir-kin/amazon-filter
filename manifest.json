{
  "name": "Amazon Filter",
  "version": "1.1",
  "description": "Filters small prices on amazon goods page",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png"
    }
  },
  "permissions": ["https://*.amazon.com/*",
        "storage",
        "tabs"
    ]
}
