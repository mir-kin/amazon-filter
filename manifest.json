{
  "name": "Amazon Filter",
  "version": "1.1",
  "description": "Filters small prices on amazon goods page",
  "manifest_version": 2,
  "content_scripts": [{
          "matches": ["<all_urls>"],
          "all_frames": true,
          "js":      ["content.js"]
      }],

  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png"
    },
    "default_popup": "popup.html"
  },
  "optional_permissions": [ "tabs", "https://www.amazon.com/" ]
}
